{
      "headers": [
        {
          "source": "/favicon.ico",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=31536000, immutable"
            }
          ]
        },
        {
          "source": "/favicon-32x32.png",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=31536000, immutable"
            }
          ]
        },
        {
          "source": "/apple-touch-icon",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=31536000, immutable"
            }
          ]
        },
        {
          "source": "/(.*)",
          "headers": [
            {
              "key": "X-Custom-Favicon",
              "value": "/favicon.ico"
            }
          ]
        }
      ],
      "rewrites": [
        {
          "source": "/favicon.ico",
          "destination": "/favicon.ico"
        }
      ],
      "redirects": [
        {
          "source": "/(apple-touch-icon.*|favicon.*(?!ico$|png$))",
          "destination": "/favicon.ico",
          "statusCode": 301
        }
      ]
    }